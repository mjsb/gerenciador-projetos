{
    "name": "mjsb/gerenciador",
    "description": "Gerenciador de Projetos",
    "authors": [
        {
            "name": "Marcio Borges",
            "email": "marciojsb@hotmail.com"
        }
    ],
    "require": {
        "pimple/pimple": "^3.2",
        "illuminate/support": "^5.6",
        "symfony/http-foundation": "^4.1",
        "zendframework/zend-eventmanager": "^3.2",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Gerenciador\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "server": "php -S localhost:8000 -t public"
    },
    "config": {
        "process-timeout": 0
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    }
}
